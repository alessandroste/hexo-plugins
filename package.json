{
  "name": "hexo-plugins",
  "version": "0.0.0",
  "private": "true",
  "description": "A set of hexo plugins",
  "scripts": {
    "generate": "npm run compile-plugins && hexo generate",
    "server": "npm run compile-plugins && hexo server --debug",
    "compile-plugins": "npm run build --workspace=packages"
  },
  "author": "alessandroste",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "docs"
  ],
  "type": "module",
  "hexo": {
    "version": "7.3.0"
  },
  "dependencies": {
    "@alessandroste/hexo-tag-pintora": "^1.0.0",
    "hexo": "^7.3.0",
    "hexo-generator-archive": "^2.0.0",
    "hexo-generator-category": "^2.0.0",
    "hexo-generator-index": "^4.0.0",
    "hexo-generator-tag": "^2.0.0",
    "hexo-renderer-ejs": "^2.0.0",
    "hexo-renderer-marked": "^6.3.0",
    "hexo-renderer-stylus": "^3.0.1",
    "hexo-server": "^3.0.0",
    "hexo-theme-landscape": "^1.0.0",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "hexo-browsersync": "^0.3.0",
    "hexo-cli": "^4.3.2",
    "typescript-eslint": "^8.10.0"
  }
}
